#make wrapper for lights out (creates matrix of possible presses depending on how large the grid is)
# 2X2 matrix: [0,0,1,0] R -> L, T -> B
from sympy import Matrix
def lightsOutWrapper(initialstateList):
    gridsize = len(initialstateList)**.5
    for X in range(0,gridsize,1):


def createMatrix(gridsize):
    matrixOfPresses = [[]]
    for X in range(1,gridsize**2,1):
        matrixOfPresses.append()

# (2, [0,1,0,0]) -> (2,2,[0,1,0,0])
def symPyInterpreter(gridsize, buttonStatusList):
    return (gridsize, gridsize, buttonStatusList).inv_mod()
